<html>
<head>
	<title><%= title %></title>
	<% include ../../partials/head %>
	<link rel="stylesheet" type="text/css" href="/assets/css/payments.css">
</head>
<body>
	<% include tenant-nav %>
	<div class="jumbotron">
		<h1><%= title %></h1>
	</div>
	<div class="container">
		<div class="row">
			<div class="col-12">
				<form data-id="<%= payment.id %>" data-uid="<%= uid %>" data-property="<%= payment.property_id %>">
					
					<% include ../../partials/payment-fields %>

					<button class="btn" type="submit"><%= title %></button>
					<a href="/tenant/home/<%= uid %>">Cancel</a>
				</form>
			</div>
		</div>
	</div>
	<script src="/scripts/payment.js"></script>
</body>
</html>