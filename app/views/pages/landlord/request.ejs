<html>
<head>
	<title><%= title %></title>
	<% include ../../partials/head %>
	<link type="text/css" rel="stylesheet" href="/assets/css/allprops.css">
	<!-- <link type="text/css" rel="stylesheet" href="/assets/css/myprops.css"> -->
</head>
<body>
	<% include landlord-nav %>
	<div class="jumbotron">
		<h1><%= title %></h1>
	</div>
	<div class="container">
		<div class="row">
			<div class="col-12">
				<form data-id="<%= request.id %>" data-uid="<%= uid %>" data-property="<%= request.property_id %>"  data-user="landlord">

					<% include ../../partials/request-fields %>
					<div class="d-flex align-items-center">
						<button class="btn btn-outline-success btn-lg dark" type="submit"><%= title %></button>
						<a class="mx-5" href="/landlord/home/<%= uid %>">Cancel</a>
					</div>
				</form>
			</div>
		</div>
	</div>
	<script src="/scripts/request.js"></script>
</body>
</html>